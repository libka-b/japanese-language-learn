{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "Japanese language learn",
  "version": "0.1.0",
  "identifier": "com.tauri.dev",
  "build": {
    "frontendDist": "../dist",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build"
  },
  "app": {
    "windows": [
      {
        "title": "Japanese language learn",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/icon.ico"
    ],
    "resources": [
      "resources/config.yaml",
      "resources/hiragana.csv",
      "resources/hiragana-plus.csv",
      "resources/katakana.csv",
      "resources/katakana-plus.csv",
      "resources/hiragana-vocabulary.csv"
    ]
  }
}
